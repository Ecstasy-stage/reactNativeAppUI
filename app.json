{
    "expo": {
        "name": "react_native_app",
        "slug": "react_native_app",
        "version": "1.0.0",
        "orientation": "default",
        "icon": "./app/assets/icon.png",
        "splash": {
        "image": "./app/assets/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
    },
    "updates": {
        "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
        "**/*"
    ],
    "ios": {
        "supportsTablet": true,
        "bundleIdentifier": "com.ecstasy.reactNativeApp",
        "googleServicesFile": "./app/config/GoogleService-Info.plist"
    },
    "android": {
        "adaptiveIcon": {
            "foregroundImage": "./app/assets/adaptive-icon.png",
            "backgroundColor": "#FFFFFF"
        },
        "package": "com.ecstasy.reactNativeApp",
        "googleServicesFile": "./app/config/google-services.json"
    },
    "web": {
        "favicon": "./app/assets/favicon.png"
    },
    "description": ""
    },
    "plugins": [
        "@react-native-firebase/app",
        "@react-native-firebase/auth"
    ]
}
